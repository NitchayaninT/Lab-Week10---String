#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main()
{
    char s1[3][30]; // you can have 3 strings. Each string can go up to 29+1 characters
    int i;
    for(i=0;i<3;i++){
        printf("Input string %d:",i);
        scanf("%s",s1[i]);
        fflush(stdin);
    
    }
    
    char first[30],last[30];
    
    strcpy(first,s1[0]);
    if(strcmp(first,s1[1])>0 ) 
           strcpy(first,s1[1]);
    if(strcmp(first,s1[2])>0 ) 
        strcpy(first,s1[2]);
    printf("%s comes first in dictionary\n",first);
    
    strcpy(last,s1[0]);
    if(strcmp(last,s1[1])<0 ) 
           strcpy(last,s1[1]);
    if(strcmp(last,s1[2])<0 ) 
        strcpy(last,s1[2]);
    printf("%s comes last in dictionary\n",last);
    
   strcat(first,last);
 
   for(i=0;i<strlen(first);i++) 
     {
         if(first[i]=='a'||first[i]=='e'||first[i]=='i'||first[i]=='o'||first[i]=='u') first[i]=toupper(first[i]);
        
     }  
    printf("%s\n",first);
  
    return 0;
}
