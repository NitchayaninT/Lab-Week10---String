#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    char str1[3][50];

    printf("Insert string : ");
    for(int i=0;i<3;i++)
    {
      scanf("%s",str1[i]);
    }
    char first[30],last[30];
    strcpy(first,str1[0]);
    strcpy(last,str1[2]);

    strcat(str1[0],str1[2]);
    int x=strlen(str1[0]);
    for(int i=x-1;i>=0;i--)
    {

         if(str1[0][i]=='a'||str1[0][i]=='e'||str1[0][i]=='i'||str1[0][i]=='o'||str1[0][i]=='u')
          {
            printf("%c",toupper(str1[0][i]));
            continue;
          }
          printf("%c",str1[0][i]);



    }

}
