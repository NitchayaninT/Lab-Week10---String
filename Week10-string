#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    char str1[50];
    char str2[50];
    char str3[50];
    printf("Insert string 1 : ");
    scanf("%s",str1);
    printf("Insert string 2 : ");
    scanf("%s",str2);
    printf("Input string 3 : ");
    scanf("%s",str3);
    strcat(str1,str3);
    int x=strlen(str1);
    for(int i=x-1;i>=0;i--)
    {
        if(str1[i]=='a'||str1[i]=='e'||str1[i]=='i'||str1[i]=='o'||str1[i]=='u')
        {

            printf("%c",toupper(str1[i]));
            continue;
        }
        printf("%c",str1[i]);
    }

}
